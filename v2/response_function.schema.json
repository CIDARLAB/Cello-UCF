{
	"$id": "https://cellocad.org/v2/gate/gate_structure.schema.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "gateStructure",
	"description": "A representation of the response function of a gate.",
	"type": "object",
	"properties": {
		"equation": {
			"type": "string",
			"description": "The name of the gate."
		},
		"tandem_efficiency_factor": {
			"description": "A color representative of the gate.",
			"type": "string"
		},
		"variables": {
			"description": "Independent variables of the equation.",
			"type": "array",
			"items": {
				"$ref": "variable.schema.json#/variable"
			}
		},
		"parameters": {
			"description": "Parameters used in the equation.",
			"type": "array",
			"items": {
				"$ref": "parameter.schema.json#/parameter"
			}
		}
	},
	"required": ["equation", "variables", "parameters"]
}
